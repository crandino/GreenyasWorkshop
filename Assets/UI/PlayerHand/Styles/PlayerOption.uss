.PlayerOption {
    flex-shrink: initial;
    flex-grow: initial;
    align-items: auto;
    flex-basis: auto;
    align-self: auto;
    justify-content: center;
    -unity-background-scale-mode: stretch-to-fill;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    background-color: rgba(88, 88, 88, 0);
    translate: 0 0;
    background-image: none;
    overflow: visible;
}
